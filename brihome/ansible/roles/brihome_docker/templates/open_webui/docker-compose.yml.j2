services:
    open_webui:
        image: ghcr.io/open-webui/open-webui:latest
        container_name: open_webui
        ports:
        - "{{ open_web_ui_port }}:8080"
        volumes:
        - open-webui:/app/backend/data
        environment:
          - OLLAMA_BASE_URL={{ ollama_base_url }}
        restart: always

volumes:
    open-webui: